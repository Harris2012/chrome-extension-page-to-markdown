{
  "manifest_version": 2,
  "name": "Page To MarkDown",
  "version": "1.0.0",
  "description": "To save cnblogs page as markdown",
  "browser_action": {
    "default_icon": "image/icon_50_50.jpg",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.cnblogs.com/*"
      ],
      "js": [
        "js/cnblogs.js"
      ]
    },
    {
      "matches": [
        "https://xueqiu.com/*"
      ],
      "js": [
        "js/jquery-1.10.2.min.js",
        "js/xueqiu.js"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "tabs",
    "activeTab"
  ]
}